{
	"extends": "../../.eslintrc.json",
	"overrides": [
		{
			"files": ["*.ts"],
			"rules": {
				"@angular-eslint/directive-selector": [
					"error",
					{
						"type": "attribute",
						"prefix": "mfe",
						"style": "camelCase"
					}
				],
				"@angular-eslint/component-selector": [
					"error",
					{
						"type": "element",
						"prefix": "mfe",
						"style": "kebab-case"
					}
				]
			}
		}
	]
}
